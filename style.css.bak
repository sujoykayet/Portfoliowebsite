*{
    box-sizing:border-box;
    padding:0;
    margin:0;
}
body{
    font-family:Arial;
    text-align:center;
	background-color:gray;
}
header{
    width:100%;
    height:52px;
    float:left;
    background-color:#000;
}
.container_fluid{
    width:100%;
    height:auto;
}
.container{
    width:100%;
    max-width:1170px;
    height:auto;
    margin:auto;
}
.background{
    width:100%;
    height:300px;
    background-image:url(background.jpg);
    background-size:cover;
    background-repeat:no-repeat;
}
.logo{
    float:left;
}
nav{
    width:500px;
    height:auto;
    float:left;
    margin-left:120px;
}
nav ul{
    list-style-type:none;
}
nav ul li{
    float:left;
}
nav ul li a{
    text-decoration:none;
    color:white;
    padding:25px;
    line-height:50px;
    font-size:20px;
}
nav ul li a.active{
   color:tomato;
}
nav ul li a:hover{
color:tomato;
}
.menu-triger{
	color:#fff;
	float:right;
	display:none;
}
.socialmedia{
    width:3%;
    float:right;
    padding:5px;
    margin-top:5px;
}
.sec1{
    width:100%;
    height:700px;
    /*background-color:gray;*/
    float:left;
}
.mypic{
    width:30%;
    margin-top:70px;
}
.name{
    color:rgba(34, 6, 100, 0.61);
    margin-top:17px;
    font-size:27px;
    font-weight: bold;
}
.pro{
    color:white;
    font-size:18px;
    margin-top:7px;
}
.carrear_Objective{
    width:20%;
    height:10px;
    background-color:rgb(236, 61, 30);
    padding:20px;
    margin-top:20px;
    line-height:2px;
    border-radius:20px;
    float:left;
    margin:50px 0 0 230px;
    color:white;
    cursor:pointer;
}
.carrear_Objective:hover{
    color:rgb(252, 231, 47);
    background-color:rgb(250, 117, 40);
}
.contact{
    width:20%;
    height:10px;
    background-color:rgb(236, 61, 30);
    padding:20px;
    margin-top:20px;
    line-height:2px;
    border-radius:20px;
    float:left;
    margin:50px 0 0 230px;
    color:rgb(255, 255, 255);
    cursor:pointer;
}
.contact:hover{
    color:rgb(219, 255, 87);
    background-color:rgb(252, 124, 39);
}
.insec1{
    float:left;
}
.info1{
    width:400px;
    height:100px;
    background-color:#000;
    float:left;
    color:rgb(255, 187, 0);
    font-size:15px;
    margin-top:10px;
    padding:10px;
    display:none;
    margin-left:140px;
}
.insec2{
    float:right;
}
.info2{
    width:300px;
    height:100px;
    background-color:#000;
    float:left;
    color:rgb(255, 175, 2);
    margin-top:10px;
    padding:10px;
    display:none;
    margin-right:210px;
}
.formbg{
    width:100%;
    height:1000px;
    float:left;
    background-image:url(myform.jpg);
    background-size:cover;
    background-repeat:no-repeat;
}
.formbg h3{
    color:gray;
    font-size:27px;
    margin-top:27px;
    border:2px solid gray;
}
form p{
    color:white;
    font-size:22px;
    margin-top:10px;
}
fieldset p{
    padding:10px;
}
legend{
    color:white;
}
input[type="text"],
input[type="email"],
textarea{
    width:100%;
    padding:2px;
    margin-top:17px;
}
.formbtn{
    width:60%;
    height:50px;
    background-color:rgb(82, 79, 79);
    padding:20px;
    font-size:20px;
    color:white;
    cursor:pointer;
    line-height:7px;
    border-radius:30px;
    border:none;
    margin-top:70px;
}
.formbtn:hover{
    background-color:rgb(170, 167, 167);
}
.sec3{
	width:100%;
	max-width:1170px;
	height:700px;
	background-image:url(Project-Management-Skills-HERO.jpg);
	background-repeat:no-repeat;
	background-size:cover;
	float:left;
	margin:40px 0;
}
.sec3 h2{
	color:rgb(250, 204, 204);
	font-size:20px;
	margin-top:17px;
	border:1px solid gray;
	padding:5px;
}
.skills h3{
    font-weight:400;
    letter-spacing:2px;
    margin:30px 0 15px 0;
    color:rgb(255, 255, 255);
    font-size:16px;
}
.progress-bar{
    width:60%;
    height:10px;
    border-radius:5px;
    background:#ffffff;
    margin-left:20%;
}
.progress-bar div{
    width:0%;
    height:10px;
    border-radius:5px;
    
}
.progress-bar div span{
    width:40px;
    height:40px;
    border-radius:50%;
    background:rgb(0, 0, 0);
    float:right;
    margin-top:-15px;
    margin-bottom:-20px;
    color:#fff;
    display:flex;
    align-items:center;
    justify-content:center;
}
.html{
    background:rgb(255, 0, 0);
    animation:html 1s linear forwards;
}
.css{
    background:blue;
    animation:css 1s linear forwards;
}
.javascript{
    background:rgb(217, 255, 0);
    animation:javascript 1s linear forwards;
}
.react{
    background:rgb(162, 0, 255);
    animation:react 1s linear forwards;
}
.python{
    background:rgb(0, 238, 255);
    animation:python 1s linear forwards;
}
.java{
    background:rgb(255, 0, 179);
    animation:java 1s linear forwards;
}
.php{
    background:rgb(43, 255, 0);
    animation:php 1s linear forwards;
}
.mysql{
    background:rgb(255, 115, 0);
    animation:mysql 1s linear forwards;
}
.html span{
    border:1px solid rgb(255, 0, 0);
}
.css span{
    border:1px solid rgb(25, 0, 255);
}
.javascript span{
    border:1px solid rgb(238, 255, 0);
}
.react span{
    border:1px solid rgb(162, 0, 255);
}
.python span{
    border:1px solid rgb(0, 225, 255);
}
.java span{
    border:1px solid rgb(255, 0, 221);
}
.php span{
    border:1px solid rgb(51, 255, 0);
}
.mysql span{
    border:1px solid rgb(255, 81, 0);
}
@keyframes html{
    100%{
        width:80%;
    }
}
@keyframes css{
    100%{
        width:70%;
    }
}
@keyframes javascript{
    100%{
        width:50%;
    }
}
@keyframes react{
    100%{
        width:60%;
    }
}
@keyframes python{
    100%{
        width:50%;
    }
}
@keyframes java{
    100%{
        width:50%;
    }
}
@keyframes php{
    100%{
        width:50%;
    }
}
@keyframes mysql{
    100%{
        width:40%;
    }
}
footer{
    width:100%;
    height:50px;
    float:left;
    background:#000;
}
footer p{
    color:rgb(255, 193, 60);
    line-height:50px;
}
@media only screen and (max-width:1024px){
	.carrear_Objective{
		margin-left:200px;
	}
	.info1{
		width:370px;
		margin-left:120px;
	}
	.info2{
		width:270px;
		float:right;
		margin-right:150px;
	}
	nav{
		width:350px;
		height:41px;
		margin-left:80px;
	}
	nav ul li a{
		font-size:18px;
		padding:14px;
		line-height:45px;
	}
	.socialmedia{
		margin-right:5px;
	}
}
@media only screen and (max-width:800px){
	header{
		height:41px;
		/* background-color:gray; */
	}
	.logo{
		margin: 3.5px 0;
		max-width:150px; height:auto;
	}
	.logo img{
		width:100%;
		height:auto;
	}
	.background{
		width:100%;
		height:500px;
	}
	.name{
		color:pink;
	}
	nav{
		width:400px;
		height:41px;
		margin-left:80px;
	}
	nav ul li a{
		font-size:18px;
		padding:17px;
		line-height:40px;
	}
	.socialmedia{
		width:4%;
	}
	.carrear_Objective{
		height:50px;
		margin:50px 0 0 120px;
		line-height:15px;
		padding-top:10px;
	}
	.contact{
		height:50px;
		margin:50px 0 0 230px;
		padding-top:25px;
	}
	.insec1{
		float:left;
		/* background-color:orange; */
	}
	.info1{
    width:220px;
    height:200px;
    margin-left:100px;
	}
	.insec2{
		width:400px;
		height:auto;
		float:right;
		/* background-color:pink; */
	}
	.info2{
    width:220px;
    height:200px;
	font-size:14px;
	padding-top:22px;
	margin-left:90px;
	}
}
@media only screen and (max-width:768px){
	.socialmedia{
		width:4%;
	}
	nav{
		width:370px;
	}
	.info2{
		margin-right:85px;
		float:right;
	}
}
@media only screen and (max-width:500px){
	header{
		height:47px;
		/* background-color:#fff; */
	}
	.logo{
		max-width:170px;
		margin-top:5px;
	}
	.logo img{
		width:100%;
		height:auto;
	}
	nav{
		display:none;
		width:100%;
		height:auto;
		margin:0px 0 0 0px;
		z-index:1;
		position:relative;
	}
	nav ul li:hover{
		background-color:#222;
	}
	nav ul li{
		width:100%;
		border-bottom:1px solid white;
		padding:2px;
	}
	nav ul li a{
		font-size:20px;
		line-height:40px;
	}
	.menu-triger{
		display:inline;
		color:#fff;
		font-size:30px;
		float:right;
		cursor:pointer;
	}
	.socialmedia{
		width:30px;
		margin-left:2px;
		margin-top:6px;
		cursor:pointer;
	}
	.background{
		width:100%;
		height:460px;
	}
	.mypic{
		width:40%;
	}
	.carrear_Objective{
		width:100px;
		line-height:12px;
		float:left;
		font-size:12px;
		height:43px;
		margin-left:104px;
		margin-top:31px;
	}
	.contact{
		width:100px;
		height:0px;
		font-size:12px;
		margin-top:30px;
		float:left;
		line-height:0px;
		margin-left:25px;
		/* background:pink; */
	}
	.info1{
		width:100%;
		margin:5px 5px;
	}
	.info2{
		width:100%;
		margin:5px 5px;
	}
@media only screen and (max-width:375px){
	.carrear_Objective{
		margin-left:80px;
	}
	.info2{
		width:360px;
		height:170px;
		font-size:14px;
		padding-top:22px;
		margin-right:10px;
		float:right;
	}
}
@media only screen and (max-width:320px){
	.carrear_Objective{
		margin-left:50px;
	}
	.info1{
		width:300px;
		height:170px;
		margin-left:100px;
		float:left;
		font-size:14px;
		margin-left:10px;
	}
	.info2{
		width:300px;
	}
	.socialmedia{
		width:20px;
		margin-top:12px;
	}
	.menu-triger{
		width:25px;
	}
}
}		 